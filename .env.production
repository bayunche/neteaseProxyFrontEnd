# 生产环境配置
NODE_ENV=production
PORT=3001

# CORS允许的来源（逗号分隔）
ALLOWED_ORIGINS=*

# 日志级别
LOG_LEVEL=info

# 是否启用请求日志
ENABLE_REQUEST_LOGGING=true

# 代理超时时间（毫秒）
PROXY_TIMEOUT=30000

# 速率限制配置
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000
PROXY_RATE_LIMIT_WINDOW_MS=60000
PROXY_RATE_LIMIT_MAX_REQUESTS=100

# 监控配置
ENABLE_METRICS=true

# 服务信息
SERVICE_VERSION=1.0.0
SERVICE_NAME=netease-proxy-server