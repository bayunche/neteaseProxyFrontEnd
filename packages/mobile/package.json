{
  "name": "@music-player/mobile",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "prebuild:shared": "cd ../shared && npm run build",
    "build:development": "eas build --platform all --profile development",
    "build:preview": "eas build --platform all --profile preview",
    "build:production": "eas build --platform all --profile production",
    "build:development:android": "eas build --platform android --profile development",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:production:android": "eas build --platform android --profile production",
    "build:development:ios": "eas build --platform ios --profile development",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:production:ios": "eas build --platform ios --profile production",
    "build:all": "npm run build:development && npm run build:preview && npm run build:production",
    "submit:production": "eas submit --profile production",
    "submit:preview": "eas submit --profile preview",
    "update:development": "eas update --branch development",
    "update:preview": "eas update --branch preview",
    "update:production": "eas update --branch production",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "install:eas": "npm install -g @expo/eas-cli",
    "setup:eas": "eas login && eas build:configure"
  },
  "dependencies": {
    "@music-player/shared": "file:../shared",
    "@react-native-community/cli-server-api": "^20.0.0",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@shopify/flash-list": "1.7.1",
    "eas": "^0.1.0",
    "expo": "~51.0.0",
    "expo-av": "~15.0.1",
    "expo-blur": "~14.0.1",
    "expo-dev-client": "~4.0.29",
    "expo-linear-gradient": "~14.0.1",
    "expo-media-library": "~17.0.3",
    "expo-notifications": "~0.30.1",
    "expo-updates": "~0.25.28",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "~4.12.0",
    "react-native-screens": "~4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.2.79",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^9.30.1",
    "expo-build-properties": "~0.12.5",
    "typescript": "~5.8.3"
  }
}
